  "name": "api",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-21",
  "compatibility_flags": ["nodejs_compat"],
  "env": {
    "dev": {
      "vars": {
        "ENV_NAME": "dev",
        "ALLOWED_DISCORD_SERVER_ID": "834383470021050399",
