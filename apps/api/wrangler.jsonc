      }
    },
    "production": {
      "vars": {
        "ENV_NAME": "production",
        "ALLOWED_DISCORD_SERVER_ID": "834383470021050399",
        // TODO:
        "BASE_URL": "",
        "FRONTEND_BASE_URL": "",
        "DATABASE_URL": "prisma+postgres://...",
        "DISCORD_CLIENT_ID": "123456789",
        "DISCORD_CLIENT_SECRET": "secret_here"
      }
